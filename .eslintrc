{
  "extends": "airbnb",
  "rules": {
    "no-console": "warn",
    "global-require": "warn",
    "no-unused-vars": "warn",
    "no-undef": "warn",
    "no-empty": "off",
    "no-await-in-loop": "off",
    "no-return-assign": "off",
    "consistent-return": "off",
    "no-use-before-define": "off"
  }
}